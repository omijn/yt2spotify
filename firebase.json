{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*"
    ],
    "rewrites": [
      {
        "source": "/convert",
        "run": {
          "serviceId": "yt2spotify",
          "region": "us-west2"
        }
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
